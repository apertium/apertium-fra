SELECT V IF (-1C Adv) (0 Fait) ;
REMOVE PP IF (-1 Ne) (0 Fait) (1 Que) ;
  ## ne fait que

REMOVE N IF (0 Est) (-1 Interr OR Rel) ;
REMOVE A IF (0 Est) (-1 Interr OR Rel) ;
