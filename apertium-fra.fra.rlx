SET Top = (np top) ;
SET Al = (al) ;
SET Acr = (acr) ;

SET DetDef = (det def) ;
SET DetInd = (det ind) ;

SET QUI-QUE = Que | Qui ;

SET CLB = Subj | Cm ;

SET Noun_Adj = N OR A ;
SET Noun_Adj_PP = N OR A OR PP ;
SET Adj_PP = A OR PP ;
